{
  "name": "unmanic-webui",
  "version": "1.0.0",
  "private": true,
  "description": "Unmanic web frontend",
  "author": "Josh.5",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Unmanic/unmanic.git"
  },
  "bugs": {
    "url": "https://github.com/Unmanic/unmanic/issues"
  },
  "homepage": "https://unmanic.app",
  "engines": {
    "node": ">=14.17.2",
    "npm": ">=6.14.13"
  },
  "scripts": {
    "lint": "eslint src/js",
    "lint:fix": "npm run lint -- --fix",
    "purge": "purgecss --config purgecss.config.js --output dist/css",
    "build": "webpack --progress --mode production --config webpack.config.js",
    "serve": "cross-env NODE_ENV=dev webpack serve --progress --config webpack.config.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "bootstrap": "^4.3.1",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.1",
    "jquery": "^3.5.1",
    "jquery.uniform": "^4.3.0",
    "popper.js": "^1.15.0",
    "simple-line-icons": "^2.5.5",
    "socicon": "^3.0.5",
    "ui-progress-circle": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^7.0.3",
    "css-loader": "^3.0.0",
    "css-url-relative-plugin": "^1.1.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-jquery": "^2.6.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^4.0.0",
    "glob": "^7.1.7",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^2.7.0",
    "image-webpack-loader": "^5.0.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "~6.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "^2.3.2",
    "prettier-package-json": "^2.6.0",
    "purgecss": "^4.0.3",
    "sass": "^1.34.1",
    "sass-loader": "^10.1.1",
    "terser-webpack-plugin": "^1.4.5",
    "url-loader": "^2.0.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
